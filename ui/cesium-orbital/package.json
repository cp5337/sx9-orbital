{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm:safe && tsc -b && vite build",
    "build:wasm": "cd beam-patterns-wasm && wasm-pack build --target web --out-dir ../src/wasm/pkg",
    "build:wasm:safe": "(command -v wasm-pack >/dev/null 2>&1 && npm run build:wasm) || echo 'Skipping WASM build - wasm-pack not installed'",
    "build:wasm:dev": "cd beam-patterns-wasm && wasm-pack build --dev --target web --out-dir ../src/wasm/pkg",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "seed": "tsx src/scripts/seedData.ts",
    "seed:beam-patterns": "tsx src/scripts/seedBeamPatterns.ts"
  },
